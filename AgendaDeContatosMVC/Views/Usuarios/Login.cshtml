@model AgendaDeContatosMVC.Models.Usuarios;

@{

    ViewData["Title"] = "Login";
}


<h1>@ViewData["Title"]</h1>

<main>
<section>
    <form method="post" action="http://localhost:5188/Usuarios/Login" id="form">
        <div class="col">
            <div class="col">
            <input type="email" class="form-control" placeholder="Email" name="Email">
            </div>
            <div class="col">
            <input type="password" class="form-control" placeholder="Senha" name="Senha" id="InputSenha">
            </div>
        </div>
            <button type="submit" class="btn btn-success" id="buttonSubmit">Enviar</button>
    </form>
</section>
</main>

